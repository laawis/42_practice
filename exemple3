			FIRST WORD

#include <unistd.h>

int	main(int ac, char **av)
{
	int	i;

	i = 0;
	if (ac == 2)
	{
		while (av[1][i] == ' ' || av[1][i] =='\t')
			i++;
		while (av[1][i] != '\0' && av[1][i] != ' ' && av[1][i] != '\t')
		{
			write(1, &str[1][i], 1);
			i++;
		}
	}
	write(1, "\n", 1);
	return (0);
}

			LAST WORD

#inlude <unistd.h>

int	main(int ac, char **av)
{
	int	i;

	i = 0;
	if (ac == 2)
	{
		while (av[1][i])
			i++;
		i--;
		while (av[1][i] <= 32)
			i--;
		while (av[1][i] > 32)
			i--;
		i++;
		while (av[1][i] > 32)
			write(1, &av[1][i], 1);
			i++;
	}
	write(1, "\n", 1);
	return (0);
}

			FIRST WORD 2
#include <unistd.h>

int	main(int ac, char **av)
{
	int	i;

	i = 0;
	if (ac == 2)
	{
		while (av[1][i] <= 32;
			i++;
		while (av[1][i] > 32)
			write (1, &av[1][i], 1);
			i++;
	}
}
	
		
